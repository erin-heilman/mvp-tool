{
  "name": "mvp-tool",
  "version": 2,
  "builds": [
    {
      "src": "api/sheets/[sheet].js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/sheets/(.*)",
      "dest": "/api/sheets/[sheet].js"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        }
      ]
    }
  ]
}